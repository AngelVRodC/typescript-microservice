{
  "name": "typescript-microservice",
  "version": "0.0.1",
  "description": "Typescript Microservice Boilerplate",
  "engineStrict": true,
  "devDependencies": {
    "@types/async": "^2.0.50",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/moment-timezone": "^0.5.10",
    "@types/node": "^10.12.10",
    "prettier": "1.15.2",
    "rimraf": "~2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.16.0",
    "tslint-microsoft-contrib": "~5.2.1",
    "tsutils": "^3.5.1",
    "typescript": "~3.1.6"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "start": "ts-node ./src/main.ts",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "ENV_LEVEL=test mocha --require ts-node/register ./test/**/*.ts",
    "serve": "tsc && nodemon --inspect ./dist/index.js --watch dist",
    "seed": "node ./dist/fixtures/index.js",
    "local-seed": "ts-node ./src/fixtures/index.ts"
  },
  "author": "Angel Rodriguez <arodriguez2552@gmail.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-interface": "^2.0.3",
    "class-validator": "^0.9.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
    "moment-timezone": "^0.5.23",
    "nodemon": "^1.18.9",
    "pg": "^7.8.0",
    "reflect-metadata": "^0.1.12",
    "tslib": "^1.9.3",
    "typeorm": "^0.3.0-alpha.22"
  }
}
